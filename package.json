{
  "name": "programmer-roadblocks",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:loc:dev": "concurrently \"npm run loc:data:dev\" \"npm run loc:dev\"",
    "start:dev": "ng serve --configuration dev",
    "start:sit": "ng serve --configuration sit",
    "start:stg": "ng serve --configuration stg",
    "start:prd": "ng serve --configuration prd",
    "loc:data:dev": "json-server --watch data/db.json",
    "loc:dev": "ng serve --configuration loc-dev --proxy-config proxy.conf.json",
    "build": "ng build",
    "build:dev": "ng build --configuration dev",
    "build:sit": "ng build --configuration sit",
    "build:qat": "ng build --configuration qat",
    "build:stg": "ng build --configuration stg",
    "build:prd": "ng build --configuration prd",
    "watch": "ng build --watch --configuration dev",
    "test": "ng test --browsers=ChromeHeadless",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@adobe/css-tools": "^4.4.4",
    "@angular/animations": "^20.3.6",
    "@angular/common": "^20.3.6",
    "@angular/compiler": "^20.3.6",
    "@angular/core": "^20.3.6",
    "@angular/forms": "^20.3.6",
    "@angular/platform-browser": "^20.3.6",
    "@angular/platform-browser-dynamic": "^20.3.6",
    "@angular/router": "^20.3.6",
    "@primeuix/themes": "^1.2.5",
    "chart.js": "^4.5.1",
    "font-awesome": "^4.7.0",
    "primeicons": "^7.0.0",
    "primeng": "^20.2.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-eslint/builder": "20.4.0",
    "@angular-eslint/eslint-plugin": "20.4.0",
    "@angular-eslint/eslint-plugin-template": "20.4.0",
    "@angular-eslint/schematics": "20.4.0",
    "@angular-eslint/template-parser": "20.4.0",
    "@angular/build": "^20.3.6",
    "@angular/cli": "^20.3.6",
    "@angular/compiler-cli": "^20.3.6",
    "@types/jasmine": "~5.1.12",
    "@typescript-eslint/eslint-plugin": "8.46.1",
    "@typescript-eslint/parser": "8.46.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.38.0",
    "jasmine-core": "^5.11.0",
    "json-server": "^0.17.4",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "^5.9.3"
  }
}
