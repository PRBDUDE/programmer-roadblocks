{
  "name": "programmer-roadblocks",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:local:development": "concurrently \"npm run local:data:development\" \"npm run local:development\"",
    "start:development": "ng serve --configuration development",
    "start:sit": "ng serve --configuration sit",
    "start:stage": "ng serve --configuration stage",
    "start:production": "ng serve --configuration production",
    "local:data:development": "json-server --watch data/db.json",
    "local:development": "ng serve --configuration local-development --proxy-config proxy.conf.json",
    "build": "ng build",
    "build:development": "ng build --configuration development",
    "build:sit": "ng build --configuration sit",
    "build:qat": "ng build --configuration qat",
    "build:stage": "ng build --configuration stage",
    "build:production": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --browsers=ChromeHeadless",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@adobe/css-tools": "^4.4.4",
    "@angular/animations": "^20.3.2",
    "@angular/common": "^20.3.2",
    "@angular/compiler": "^20.3.2",
    "@angular/core": "^20.3.2",
    "@angular/forms": "^20.3.2",
    "@angular/platform-browser": "^20.3.2",
    "@angular/platform-browser-dynamic": "^20.3.2",
    "@angular/router": "^20.3.2",
    "@primeuix/themes": "^1.2.3",
    "chart.js": "^4.5.0",
    "font-awesome": "^4.7.0",
    "primeicons": "^7.0.0",
    "primeng": "^20.2.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-eslint/builder": "20.3.0",
    "@angular-eslint/eslint-plugin": "20.3.0",
    "@angular-eslint/eslint-plugin-template": "20.3.0",
    "@angular-eslint/schematics": "20.3.0",
    "@angular-eslint/template-parser": "20.3.0",
    "@angular/build": "^20.3.3",
    "@angular/cli": "^20.3.3",
    "@angular/compiler-cli": "^20.3.2",
    "@types/jasmine": "~5.1.9",
    "@typescript-eslint/eslint-plugin": "8.45.0",
    "@typescript-eslint/parser": "8.45.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "jasmine-core": "^5.11.0",
    "json-server": "^0.17.4",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "^5.9.3"
  }
}
