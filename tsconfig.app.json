/* To learn more about this file see: https://angular.dev/reference/configs/angular-compiler-options. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "baseUrl": "./",
    "paths": {
      "@utility/*": [
        "./src/app/core/utility/css-property/*",
        "./src/app/core/utility/debug-output-card/*",
        "./src/app/core/utility/functions/*"
      ],
      "@rootComponents/*": [
        "./src/app/core/roadblock-header/*",
        "./src/app/core/roadblock-footer/*"
      ],
      "@filters/*": [
        "./src/app/core/filter-types/*"
      ],
      "@errorHandlers/*": [
        "./src/app/core/error-handlers/input-error/*"
      ]
    }
  },
  "files": [
    "src/main.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
